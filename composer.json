{
  "name": "cspray/annotated-console",
  "description": "An abstraction layer over Symfony Console to easily use Annotated Container in your CLI apps.",
  "require": {
    "php": "^8.1",
    "cspray/annotated-container": "dev-main",
    "symfony/console": "^6.1",
    "rdlowrey/auryn": "^1.4"
  },
  "autoload": {
    "psr-4": {
      "Cspray\\AnnotatedConsole\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Cspray\\AnnotatedConsole\\": "test",
      "Cspray\\AnnotatedConsoleDemo\\": "demo"
    }
  },
  "require-dev": {
    "phpunit/phpunit": "^9.5"
  }
}
